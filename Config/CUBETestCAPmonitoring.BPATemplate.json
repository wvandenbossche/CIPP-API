{
  "name": "CUBE - Test CAP monitoring",
  "style": "Table",
  "Fields": [
    {
      "name": "cubeCAPexist",
      "UseExistingInfo": false,
      "FrontendFields": [
        {
          "name": "Cube CAP rules exist",
          "value": "cubeCAPexist",
          "formatter": "bool"
        }
      ],
      "where": "'VPN: Require MFA outside the office' -in $_.displayName",
      "StoreAs": "bool",
      "API": "Graph",
      "ExtractFields": [
        "displayName"
      ],
      "URL": "https://graph.microsoft.com/v1.0/policies/conditionalAccessPolicies"
    },
    {
      "name": "cubeMFAenabled",
      "UseExistingInfo": false,
      "FrontendFields": [
        {
          "name": "Cube CAP MFA enabled",
          "value": "cubeMFAenabled",
          "formatter": "bool"
        }
      ],
      "where": "$_.state -eq 'enabled'",
      "StoreAs": "bool",
      "API": "Graph",
      "ExtractFields": [
        "displayName",
        "state"
      ],
      "URL": "https://graph.microsoft.com/v1.0/policies/conditionalAccessPolicies?$filter=displayName eq 'All apps: Require MFA outside the office'"
    }
  ]
}
